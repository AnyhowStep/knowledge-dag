{
  "name": "@anyhowstep/knowledge-dag",
  "version": "0.0.0",
  "description": "Represent knowledge with a directed acyclic graph",
  "main": "index.js",
  "scripts": {
    "clean": "(rm -r ./dist) || true",
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "migrate-up-to-latest": "ts-node -P ./src/migration/tsconfig.json ./src/migration/migrate-up-to-latest.ts",
    "migrate-down": "ts-node -P ./src/migration/tsconfig.json ./src/migration/migrate-down.ts",
    "validate-schema": "ts-node -P ./src/migration/tsconfig.json ./src/migration/validate-schema.ts",
    "test": "ts-node -P ./test/run-time/tsconfig.json -r ./test/run-time/tape-stream.ts ./test/run-time/runner.ts",
    "webpack-dev": "WEBPACK_MODE=development webpack --config ./webpack.config.ts --mode=development",
    "webpack-prod": "WEBPACK_MODE=production webpack --config ./webpack.config.ts --mode=production",
    "start": "ts-node -P ./src/server-main/tsconfig.json ./src/server-main/index.ts",
    "start-dev": "ENV=./development.env npm run start",
    "pm2-start": "./node_modules/.bin/pm2 start ./dist/server-main/index.js --name=knowledge-dag",
    "pm2-stop": "./node_modules/.bin/pm2 stop knowledge-dag",
    "pm2-restart": "./node_modules/.bin/pm2 restart knowledge-dag",
    "pm2-log": "./node_modules/.bin/pm2 log",
    "pm2-start-dev": "ENV=./development.env npm run pm2-start",
    "pm2-restart-dev": "ENV=./development.env npm run pm2-restart"
  },
  "author": "anyhowstep",
  "license": "MIT",
  "devDependencies": {
    "@anyhowstep/typed-env": "^2.1.1",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.6",
    "@types/lz-string": "^1.3.34",
    "@types/mysql": "^2.15.10",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/tape": "^4.13.0",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "agnostic-migrator": "^1.0.0",
    "classnames": "^2.2.6",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-plugin-local": "^1.0.0",
    "file-loader": "^6.0.0",
    "flavormark": "^1.1.3",
    "img-loader": "^3.0.1",
    "lz-string": "^1.4.4",
    "pm2": "^4.4.0",
    "querystring-browser": "^1.0.4",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dom-factories": "^1.0.2",
    "react-router-dom": "^5.1.2",
    "react-selectize": "^3.0.1",
    "react-transition-group": "^1.1.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "tape": "^5.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.9.1",
    "typescript": "^3.5.1",
    "url-loader": "^0.6.2",
    "vis": "^4.21.0-EOL",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@squill/mysql-5.7": "0.0.11",
    "@squill/squill": "0.0.8",
    "bigint-lib": "^1.1.0",
    "route-client": "^0.2.3",
    "route-declaration": "^0.3.2",
    "route-express": "^0.2.5",
    "type-mapping": "^1.32.0"
  }
}
